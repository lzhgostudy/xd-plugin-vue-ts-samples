{
    "manifestVersion": 4,
    "name": "Hello XD Plugin",
    "host": {
      "app": "XD",
      "minVersion": "36.0.0"
    },
    "id": "hello-xd-plugin",
    "icons": [
      {
        "path": "images/icon@1x.png",
        "width": 24,
        "height": 24
      },
      {
        "path": "images/icon@2x.png",
        "width": 48,
        "height": 48
      },
      {
        "path": "images/icon@3x.png",
        "width": 96,
        "height": 96
      },
      {
        "path": "images/icon@4x.png",
        "width": 144,
        "height": 144
      },
      {
        "path": "images/icon@5x.png",
        "width": 192,
        "height": 192
      }
    ],
    "main": "main.js",
    "entrypoints": [
      {
        "type": "command",
        "id": "helloVue",
        "label": "Hello Vue2.7"
      },
      {
        "type": "command",
        "id": "createLinesCommand",
        "label": "Draw Lines"
      },
      {
        "type": "command",
        "id": "exportRendition",
        "label": "Export A Rendition"
      },
      {
        "type": "panel",
        "id": "imagePlaceholder",
        "label": "Image Placeholder"
      }
    ],
    "version": "0.0.1"
  }
  